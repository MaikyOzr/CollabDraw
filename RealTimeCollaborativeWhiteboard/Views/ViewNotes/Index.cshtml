@model IEnumerable<RealTimeCollaborativeWhiteboard.Models.Notes>
@{
    ViewBag.Title = "Test Desk";
}

<h1>Notes page</h1>
<table class="table">
    <tr><th>Name</th><th>Content</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td>@item.Content</td>
        </tr>    

    }
</table>


